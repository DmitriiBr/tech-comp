{
    "name": "turborepo-basic",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "vite-host",
        "vite-app",
        "webpack-app"
    ],
    "devDependencies": {
        "prettier": "^2.5.1",
        "turbo": "^0.9.0-next.6"
    },
    "scripts": {
        "dev": "turbo run dev"
    },
    "turbo": {
        "pipeline": {
            "build": {
                "dependsOn": [
                    "^build"
                ],
                "outputs": [
                    "dist/**"
                ]
            },
            "lint": {
                "outputs": []
            },
            "dev": {
                "cache": false
            }
        }
    },
    "dependencies": {
        "turborepo-basic": "link:",
        "vite": "^7.1.3"
    }
}
